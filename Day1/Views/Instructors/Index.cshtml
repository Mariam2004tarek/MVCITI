@using Day1.Controllers
@model List<Instructor>

<table class="table table-bordered table-hover">
    @foreach (var i in Model)
    {
        <tr>
            <td>@i.Id</td>
            <td>@i.Name</td>
            <td>@i.Salary</td>
            <td>@i.Address</td>
            <td>@i.CrsId</td>
            <td>@i.DeptId</td>
        </tr>
    }
</table>