@using Day1.Controllers
@model List<Course>

<table class="table table-bordered table-hover">
    @foreach(var i in Model)
    {
        <tr>
            <td>@i.Id</td>
            <td>@i.Name</td>
            <td>@i.Degree</td>
            <td>@i.MinimumDegree</td>
            <td>@i.Hours</td>
            <td>@i.DeptId</td>
        </tr>
    }
</table>
